@import "mixi.styl"
*
  margin 0
  padding 0
a
  text-decoration none
  color #000
#app
  position absolute
  top 0
  left 0
  right 0
  bottom 0
  width 500px
  font-family '微软雅黑'
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  font-size 16px
  margin 0% auto
  overflow hidden
@media screen and (max-width 768px)
  #app
    width 100%
    border none
.border-1px
    height 100%
    border-left: 1px solid #2CA2B4
    border-right: 1px solid #2CA2B4
.backgroundColor
  background-color #2CA2B4
  color #fff
